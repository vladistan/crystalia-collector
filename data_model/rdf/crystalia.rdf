<https://crystalia.link/ontology/1.0/Item> a linkml:ClassDefinition ;
    skos:definition "An individual item for example a file" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/Item> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/Item> ;
    linkml:is_a <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/hasDescriptor>,
        <https://crystalia.link/ontology/1.0/id>,
        <https://crystalia.link/ontology/1.0/isPartOf>,
        <https://crystalia.link/ontology/1.0/label> .

<https://crystalia.link/ontology/1.0/hasDescriptor> a linkml:SlotDefinition ;
    skos:definition "The descriptors associated with an item" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/hasDescriptor> ;
    linkml:domain_of <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:multivalued true ;
    linkml:owner <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:range <https://crystalia.link/ontology/1.0/Descriptor> ;
    linkml:slot_uri <https://crystalia.link/ontology/1.0/hasDescriptor> .

<https://crystalia.link/ontology/1.0/label> a linkml:SlotDefinition ;
    skos:definition "A human-readable label",
        "A human-readable label for the item, most often the filename" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    skos:mappingRelation rdfs:label ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/label> ;
    linkml:domain_of <https://crystalia.link/ontology/1.0/DescriptorType>,
        <https://crystalia.link/ontology/1.0/Item>,
        <https://crystalia.link/ontology/1.0/Method> ;
    linkml:owner <https://crystalia.link/ontology/1.0/Method> ;
    linkml:range <https://crystalia.link/ontology/1.0/string> ;
    linkml:required true ;
    linkml:slot_uri rdfs:label .

<https://crystalia.link/ontology/1.0/string> a linkml:TypeDefinition ;
    skos:definition "A character string" ;
    skos:editorialNote "In RDF serializations, a slot with range of string is treated as a literal or type xsd:string.   If you are authoring schemas in LinkML YAML, the type is referenced with the lower case \"string\"." ;
    skos:exactMatch schema1:Text ;
    skos:inScheme linkml:types ;
    linkml:base "str" ;
    linkml:definition_uri linkml:String ;
    linkml:imported_from "linkml:types" ;
    linkml:uri xsd:string .

<https://crystalia.link/ontology/1.0/DescribableThing> a linkml:ClassDefinition ;
    skos:definition "An item that can be described by one or more descriptors. Could be an item(file) or another descriptor" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:is_a <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/hasDescriptor>,
        <https://crystalia.link/ontology/1.0/id> .

<https://crystalia.link/ontology/1.0/Thing> a linkml:ClassDefinition ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/id> .

<https://crystalia.link/ontology/1.0/DescriptorType> a linkml:ClassDefinition ;
    skos:definition "Details about a descriptor type" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/DescriptorType> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/DescriptorType> ;
    linkml:is_a <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/id>,
        <https://crystalia.link/ontology/1.0/label>,
        <https://crystalia.link/ontology/1.0/max_block_size>,
        <https://crystalia.link/ontology/1.0/usesMethod> .

<https://crystalia.link/ontology/1.0/id> a linkml:SlotDefinition ;
    skos:definition "A unique identifier" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/id> ;
    linkml:domain_of <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:identifier true ;
    linkml:owner <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:range <https://crystalia.link/ontology/1.0/uriorcurie> ;
    linkml:required true ;
    linkml:slot_uri <https://crystalia.link/ontology/1.0/id> .

<https://crystalia.link/ontology/1.0/Method> a linkml:ClassDefinition ;
    skos:definition "A method used to generate a descriptor" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/Method> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/Method> ;
    linkml:is_a <https://crystalia.link/ontology/1.0/Thing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/comment>,
        <https://crystalia.link/ontology/1.0/id>,
        <https://crystalia.link/ontology/1.0/label>,
        <https://crystalia.link/ontology/1.0/robustness> .

<https://crystalia.link/ontology/1.0/Descriptor> a linkml:ClassDefinition ;
    skos:definition "A descriptor for the whole or part of an item or another descriptor" ;
    skos:inScheme <https://crystalia.link/ontology> ;
    linkml:attributes <https://crystalia.link/ontology/1.0/label> ;
    linkml:class_uri <https://crystalia.link/ontology/1.0/Descriptor> ;
    linkml:definition_uri <https://crystalia.link/ontology/1.0/Descriptor> ;
    linkml:is_a <https://crystalia.link/ontology/1.0/DescribableThing> ;
    linkml:slot_usage [ ] ;
    linkml:slots <https://crystalia.link/ontology/1.0/coverage>,
        <https://crystalia.link/ontology/1.0/descriptor__label>,
        <https://crystalia.link/ontology/1.0/hasDescriptor>,
        <https://crystalia.link/ontology/1.0/hasType>,
        <https://crystalia.link/ontology/1.0/id>,
        <https://crystalia.link/ontology/1.0/length>,
        <https://crystalia.link/ontology/1.0/offset>,
        <https://crystalia.link/ontology/1.0/value> .


